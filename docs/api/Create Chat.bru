meta {
  name: Create Chat
  type: http
  seq: 1
}

post {
  url: {{host}}/chat
  body: json
  auth: bearer
}

headers {
  x-user-email: nick@undefined.computer
}

auth:bearer {
  token: {{token}}
}

body:json {
  {
    "chat_id": "test",
    "input": "Can you explain the basics of machine learning?",
    "model": "Phi-3.5-MoE-instruct",
    "temperature": 0.8,
    "max_tokens": 2048,
    "top_p": 0.1,
    "useRAG": false,
    "shouldSave": false,
    "budgetConstraint": 0.01
  }
}
