name="assistant"
main="src/index.ts"

compatibility_date = "2022-11-22"

kv_namespaces = [
  {binding = "assistant_chats",id = "<id>",preview_id = "<preview_id>"}
]

[ai]
binding = "AI"

[observability]
enabled = true
head_sampling_rate = 1

[observability.logs]
enabled = true
invocation_logs = false

[dev]
port = 8787

[[r2_buckets]]
binding = 'ASSETS_BUCKET'
bucket_name = 'assistant-assets'
preview_bucket_name = 'assistant-assets-preview'

[[unsafe.bindings]]
name = "RATE_LIMITER"
type = "ratelimit"
namespace_id = "1001"
simple = { limit = 100, period = 60 }
